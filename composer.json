{
    "name": "asantibanez/laravel-workflow",
    "description": "Manage business processes workflows in a breeze",
    "keywords": [
        "asantibanez",
        "laravel-workflow"
    ],
    "homepage": "https://github.com/asantibanez/laravel-workflow",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrés Santibáñez",
            "email": "santibanez.andres@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "illuminate/support": "5.7.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Asantibanez\\LaravelWorkflow\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asantibanez\\LaravelWorkflow\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asantibanez\\LaravelWorkflow\\LaravelWorkflowServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Asantibanez\LaravelWorkflow\\LaravelWorkflowFacade"
            }
        }
    }
}
